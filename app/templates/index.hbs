<h1 id="title">W E L C O M E</h1>
{{question.question}}

<div class="question-form">
  {{new-question save="save"}}
</div>

<ul class="display-box">
  {{#each model.questions as |question|}}
    {{question-tile question=question update="update"}}
    <ul class="sub-display-box">
      {{#each model.answers key="id" as |answer|}}
        {{answer-tile answer=answer}}
      {{/each}}
     </ul>
  {{/each}}

</ul>
